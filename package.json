{
  "name": "configure-iam-bastion-credentials",
  "version": "0.1.0",
  "private": true,
  "description": "Configures the environment with AWS credentials for an IAM Bastion role",
  "main": "lib/main.js",
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "format": "prettier --write src/**/*.ts",
    "format-check": "prettier --check src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "rimraf dist && ncc build --source-map --license licenses.txt -o dist/main && ncc build lib/cleanup.js --source-map --license licenses.txt -o dist/cleanup",
    "prepare": "husky install"
  },
  "repository": "github:idealo/configure-iam-bastion-credentials",
  "keywords": [
    "actions",
    "aws",
    "iam"
  ],
  "author": "Heiko Rothe <heiko.rothe@idealo.de>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.7",
    "@actions/github": "^4.0.0",
    "typed-rest-client": "^1.8.4"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.5",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.14.1",
    "lint-staged": "^10.5.4",
    "nock": "^13.0.11",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.ts": "eslint"
  }
}
