{
    "plugins": ["jest", "@typescript-eslint"],
    "extends": [
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "import/no-namespace": "off"
    },
    "env": {
      "node": true,
      "jest/globals": true
    }
  }
